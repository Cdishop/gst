@article{naylor_test_1969,
  title = {A Test of the Progression-Regression Hypotheses in a Cognitive Inference Task},
  volume = {4},
  issn = {0030-5073},
  url = {http://www.sciencedirect.com/science/article/pii/0030507369900142},
  doi = {10.1016/0030-5073(69)90014-2},
  abstract = {The study was designed to test the validity of the progression-regression hypotheses of Fitts et al. in the context of performance on a cognitive inference task. Using parabolas as stimuli, subjects were required to make predictive responses. The correct response for each parabola was based upon an equation which involved the first and second derivatives of each stimulus figure. Two groups of 11 subjects each experienced 12 days of training followed by 2 days of transfer under mild stress followed by 5 more days of transfer under heavier stress. The groups differed in degree of specificity of instructions. Evidence for the progression hypothesis was obtained in both groups, but no evidence at all was found for the regression hypothesis under the stress conditions.},

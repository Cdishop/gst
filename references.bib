@article{komaki_behavioral_1978,
  langid = {english},
  title = {A {{Behavioral Approach}} to {{Occupational Safety}}: {{Pinpointing}} and {{Reinforcing Safe Performance}} in a {{Food Manufacturing Plant}}},
  volume = {63},
  issn = {00219010},
  url = {http://search.proquest.com/docview/213939054?pq-origsite=summon},
  shorttitle = {A {{Behavioral Approach}} to {{Occupational Safety}}},
  abstract = {The behavior analysis approach was used to improve worker safety in 2 departments in a food manufacturing plant. Desired safety practices were identified, permitting construction of observational codes suitable for observing workers' on-the-job performance over a 25-week period of time. The intervention consisted of an explanation and visual presentation of the desired behaviors, as well as frequent, low-cost reinforcement in the form of feedback. A within-subject (multiple baseline) design was used. Employees in the 2 departments substantially improved their safety performance from 70\% and 78\% to 96\% and 99\% in the wrapping and makeup departments, respectively, after the staggered introduction of the program. During the reversal phase, performance returned to baseline (71\% and 72\%). It was concluded that the intervention, particularly the frequent feedback, was effective in improving safety performance. Not only did employees react favorably to the program, but the company was later able to maintain the program with a continuing decline in the injury frequency rate. The results suggest that behaviorally defining and positively reinforcing safe practices is a visible approach to the occupational accident reduction.},
  number = {4},
  journaltitle = {Journal of Applied Psychology; Washington},
  urldate = {2018-11-22},
  date = {1978-08},
  pages = {434},
  keywords = {Psychology,Studies,Behavior,Employees,Intervention,Occupational safety,Performance,Safety},
  author = {Komaki, Judi and Barwick, Kenneth D. and Scott, Lawrence R.},
  file = {files/2035/Komaki et al. - 1978 - A Behavioral Approach to Occupational Safety Pinp.pdf}
}

@article{komaki_effect_1980,
  langid = {english},
  title = {Effect of Training and Feedback: {{Component}} Analysis of a Behavioral Safety Program},
  volume = {65},
  issn = {0021-9010},
  url = {http://search.proquest.com/docview/614330117/abstract/63B8D48DFBC24E85PQ/1},
  doi = {http://dx.doi.org.proxy1.cl.msu.edu/10.1037/0021-9010.65.3.261},
  shorttitle = {Effect of Training and Feedback},
  abstract = {Desired safety practices were behaviorally defined for 4 sections of a city's vehicle maintenance division; a total of 55 employees differing in age and seniority participated. A total of 165 observations were conducted over 45 wks. A multiple-baseline design with a reversal component was used in which 5 conditions were introduced: (a) baseline, (b) Training Only 1 (desired practices were discussed, illustrated, and posted), (c) Training and Feedback 1 (supervisors observed daily and provided feedback about the section safety level on graphs), (d) Training Only 2, and (e) Training and Feedback 2. Whereas employees showed only slight improvements during the Training Only 1 phase, their performance increased substantially during the Training and Feedback 1 phase. At this point, it was concluded that the provision of training alone is not a sufficient means of improving and maintaining performance. This conclusion was confirmed during the Training Only 2 phase when performance declined. Performance improved once again during the Training and Feedback 2 phase, but only when feedback was provided at least 3 times/wk. This qualification raises questions concerning the maintenance of performance gains and supervisory support. (31 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  number = {3},
  journaltitle = {Journal of Applied Psychology},
  urldate = {2018-11-22},
  date = {1980-06},
  pages = {261-270},
  keywords = {Feedback (major),Occupational Safety (major),Personnel Training (major)},
  author = {Komaki, Judi and Heinzmann, Arlene T. and Lawson, Loralie},
  file = {files/2037/Komaki et al. - 1980 - Effect of training and feedback Component analysi.pdf}
}

@article{komaki_behavioral_1977,
  title = {A Behavioral Approach to Coaching Football: Improving the Play Execution of the Offensive Backfield on a Youth Football Team},
  volume = {10},
  issn = {0021-8855},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC1311243/},
  doi = {10.1901/jaba.1977.10-657},
  shorttitle = {A Behavioral Approach to Coaching Football},
  abstract = {Because of the emphasis on winning, the difficulties involved in assessing performance, and the lack of frequent and contingent reinforcement, a behavioral approach to coaching football was used. The players, all nine- or ten-year-old males, were members of an offensive backfield on a Pop Warner football team. Three frequently-run offensive plays were broken down into a series of five behaviorally defined stages, permitting construction of checklists suitable for observing the players during both game and scrimmage sessions. The intervention consisted of the presentation and explanation of the appropriate checklist, and frequent contingent reinforcement in the form of feedback and recognition for instances of desired play execution. Performance gains averaging 20\% occurred for each of the three plays after, and not before, the staggered introduction of each intervention. The results suggest that behavioral specification and positive reinforcement of desired play execution is a viable approach to the coaching of football.},
  number = {4},
  journaltitle = {Journal of Applied Behavior Analysis},
  shortjournal = {J Appl Behav Anal},
  urldate = {2018-11-22},

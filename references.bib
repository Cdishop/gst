}

@article{wageman_as_2005,
  eprinttype = {jstor},
  eprint = {25146004},
  title = {As the {{Twig Is Bent}}: {{How Group Values Shape Emergent Task Interdependence}} in {{Groups}}},
  volume = {16},
  issn = {1047-7039},
  shorttitle = {As the {{Twig Is Bent}}},
  abstract = {This paper proposes that task interdependence in teams, usually treated solely as structurally driven, can arise from group members' values at the time of group formation. Findings from a longitudinal field study show that teams whose members share egalitarian values at formation develop highly interdependent task approaches and exhibit patterns of social interaction similar to those associated with high structural interdependence. By contrast, groups whose members share meritocratic values at formation develop low-interdependence task approaches and exhibit patterns of social interaction similar to those associated with low structural interdependence. Groups with shared hybrid or with mixed values show no consistent task structures or group processes, and significantly underperform relative to both the egalitarian and the meritocratic groups. We discuss the implications of these findings for the effectiveness of task-performing teams in organizations.},
  number = {6},
  journaltitle = {Organization Science},
  date = {2005},
  pages = {687-700},
  author = {Wageman, Ruth and Gordon, Frederick M.},
  file = {files/4139/Wageman and Gordon - 2005 - As the Twig Is Bent How Group Values Shape Emerge.pdf}
}


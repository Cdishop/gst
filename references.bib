  pages = {1001},
  keywords = {Psychology,Studies,Leadership,Causality,Client satisfaction,Comparative analysis,Customer satisfaction,Financial services,Propensity,Selection bias,Selection effects,Weighting},
  author = {Schmidt, Joseph A. and Pohler, Dionne M.},
  file = {files/3399/Schmidt and Pohler - 2018 - Making stronger causal inferences Accounting for .pdf}
}

@article{deortentiis_build_2018,
  langid = {english},
  title = {Build or Buy? {{The}} Individual and Unit-Level Performance of Internally versus Externally Selected Managers over Time},
  volume = {103},
  issn = {0021-9010},

  langid = {english},
  title = {Differences in Causal Estimates from Longitudinal Analyses of Residualized versus Simple Gain Scores: {{Contrasting}} Controls for Selection and Regression Artifacts},
  volume = {34},
  issn = {0165-0254, 1464-0651},

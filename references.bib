  keywords = {Psychology,Studies,Organization theory,Morality,Employee management relations,Employment security,Social exchange theory},
  author = {Huang, Guo-hua and Wellman, Ned and Ashford, Susan J. and Lee, Cynthia and Wang, Li},
  file = {files/3521/Huang et al. - 2017 - Deviance and exit The organizational costs of job.pdf}
}


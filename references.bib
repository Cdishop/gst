  pages = {1-8},
  author = {Girard, Jeffrey M. and Cohn, Jeffrey F. and Mahoor, Mohammad H. and Mavadati, Seyedmohammad and Rosenwald, Dean P.},
  file = {files/2701/Girard et al. - 2013 - Social risk and depression Evidence from manual a.pdf}
}

@article{schneider_computer-based_1990,

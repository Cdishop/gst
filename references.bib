  keywords = {Health Care Services,Health Personnel Attitudes (major),Hospitals,Human Resource Management (major),Job Satisfaction (major),Organizational Effectiveness (major)},
  author = {Piening, Erk P. and Baluch, Alina M. and Salge, Torsten Oliver},
